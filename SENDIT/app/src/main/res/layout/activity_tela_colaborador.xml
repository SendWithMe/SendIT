<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TelaColaborador">

    <Button
        android:id="@+id/btn_listaPedidosColaborador"
        android:layout_width="378dp"
        android:layout_height="48dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="#393636"
        android:onClick="listaPedidosColaborador"
        android:text="Lista de pedidos"
        android:textColor="#ed9806"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_infoPerfilColaborador"
        android:layout_width="376dp"
        android:layout_height="62dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="150dp"
        android:backgroundTint="#393636"
        android:onClick="perfilColaborador"
        android:text="Perfil"
        android:textColor="#ed9806"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_listaPedidosColaborador"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/button5"
        android:layout_width="378dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:backgroundTint="#393636"
        android:onClick="sairColaborador"
        android:text="sair"
        android:textColor="#ed9806"
        app:layout_constraintTop_toBottomOf="@+id/btn_infoPerfilColaborador"
        tools:layout_editor_absoluteX="3dp" />


</android.support.constraint.ConstraintLayout>